<?xml version="1.0" encoding="utf-8" ?>
<TabbedPage xmlns="http://xamarin.com/schemas/2014/forms"
            xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
            xmlns:views="clr-namespace:Recetas.Views"
            x:Class="Recetas.Views.MainPage">
    <TabbedPage.Children>
        <NavigationPage Title="Receta de cocina">
            <x:Arguments>
                <views:RecetaPage />
            </x:Arguments>
            <NavigationPage.Icon>
                <OnPlatform x:TypeArguments="FileImageSource">
                    <On Platform="iOS" Value="tab_feed.png"/>
                </OnPlatform>
            </NavigationPage.Icon>
        </NavigationPage>
        
    </TabbedPage.Children>
    
</TabbedPage>